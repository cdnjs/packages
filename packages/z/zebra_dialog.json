{
  "name": "zebra_dialog",
  "description": "A small, compact, and highly configurable dialog box plugin for jQuery meant to replace JavaScript's native alert and confirmation windows",
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "alert",
    "dialog",
    "modal",
    "popup",
    "ui",
    "confirmation"
  ],
  "author": "Stefan Gabos <contact@stefangabos.ro>",
  "license": "LGPL-3.0",
  "homepage": "https://github.com/stefangabos/Zebra_Dialog",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stefangabos/Zebra_Dialog.git"
  },
  "filename": "zebra_dialog.min.js",
  "autoupdate": {
    "source": "npm",
    "target": "zebra_dialog",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      }
    ]
  }
}
