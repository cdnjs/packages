{
  "name": "core-js",
  "description": "Standard library",
  "repository": {
    "type": "git",
    "url": "https://github.com/zloirock/core-js.git"
  },
  "filename": "minified.js",
  "license": "MIT",
  "keywords": [
    "ES5",
    "ECMAScript 5",
    "ES6",
    "ECMAScript 6",
    "ES7",
    "ECMAScript 7",
    "Harmony",
    "Strawman",
    "Map",
    "Set",
    "WeakMap",
    "WeakSet",
    "Dict",
    "Promise",
    "Symbol",
    "Array generics",
    "setImmediate",
    "partial application",
    "Date formatting"
  ],
  "autoupdate": {
    "source": "npm",
    "target": "core-js-bundle",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "minified.js?(.map)"
        ]
      }
    ],
    "ignoreVersions": [
      "1,0,1"
    ]
  },
  "authors": [
    {
      "name": "Denis Pushkarev",
      "email": "zloirock@zloirock.ru",
      "url": "http://zloirock.ru/"
    }
  ]
}
