{
  "name": "Camera",
  "description": "A jQuery slideshow with many effects, transitions, easy to customize, using canvas and mobile ready, based on jQuery 1.9.1+",
  "filename": "scripts/camera.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixedelic/Camera.git"
  },
  "keywords": [
    "slideshow",
    "customize",
    "canvas"
  ],
  "author": {
    "name": "Manuel Masia"
  },
  "license": "MIT",
  "homepage": "http://www.pixedelic.com/plugins/camera/",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/pixedelic/Camera.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "+(scripts|css)/camera*.+(js|css|map)",
          "scripts/jquery.mobile.customized*.js",
