{
  "name": "crypto-js",
  "description": "JavaScript library of crypto standards.",
  "repository": {
    "type": "git",
    "url": "http://github.com/brix/crypto-js.git"
  },
  "license": "MIT",
  "keywords": [
    "security",
    "crypto",
    "Hash",
    "MD5",
    "SHA1",
    "SHA-1",
    "SHA256",
    "SHA-256",
    "RC4",
    "Rabbit",
    "AES",
    "DES",
    "PBKDF2",
    "HMAC",
    "OFB",
    "CFB",
    "CTR",
    "CBC",
    "Base64"
  ],
  "filename": "crypto-js.min.js",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/brix/crypto-js.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "**/*.+(js|map)"
        ]
      }
    ],
    "ignoreVersions": [
      "release-3.1.2*"
    ]
  },
  "authors": [
    {
      "name": "Evan Vosberg",
      "url": "http://github.com/evanvosberg"
    }
  ]
}
