{
  "name": "CodeFlask.js",
  "filename": "codeflask.min.js",
  "homepage": "https://kazzkiq.github.io/CodeFlask/",
  "repository": {
    "type": "git",
    "url": "git://github.com/kazzkiq/CodeFlask.js"
  },
  "description": "A micro code-editor for awesome web pages.",
  "keywords": [
    "code-editor",
    "plugin",
    "syntax-highlight"
  ],
  "license": "MIT",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/kazzkiq/CodeFlask.js.git",
    "fileMap": [
      {
        "basePath": "build",
        "files": [
          "**/!(*.html)"
        ]
      },
      {
        "basePath": "src",
        "files": [
          "codeflask.js"
        ]
      }
    ]
  },
  "authors": [
    {
      "name": "Claudio Holanda"
    }
  ]
}
