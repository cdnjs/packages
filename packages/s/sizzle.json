    "jquery"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jquery/sizzle.git"
  },
  "license": "MIT",
  "author": {
    "name": "jQuery Foundation and other contributors",
    "url": "https://github.com/jquery/sizzle/blob/master/AUTHORS.txt"
  },
  "autoupdate": {
    "source": "npm",
    "target": "sizzle",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "*"
        ]
      }
    ]
  }
}
