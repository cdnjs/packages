{
  "name": "svg4everybody",
  "description": "Use external SVG spritemaps in any browser",
  "author": {
    "name": "Jonathan Neal <jonathantneal@hotmail.com> (http://jonathantneal.com)"
  },
  "license": "CC0-1.0",
  "keywords": [
    "svgs",
    "sprites",
    "spritemaps",
    "symbols",
    "defs",
    "uses",
    "oldies",
    "ie8s",
    "ie9s",
    "safaris",
    "externals",
    "icons",
    "fallbacks"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jonathantneal/svg4everybody.git"
  },
  "filename": "svg4everybody.min.js",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/jonathantneal/svg4everybody.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      }
    ]
  }
}
