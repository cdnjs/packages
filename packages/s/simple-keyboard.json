  "repository": {
    "type": "git",
    "url": "git+https://github.com/hodgef/simple-keyboard.git"
  },
  "authors": [
    {
      "name": "Francisco Hodge",
      "email": "hello@franciscohodge.com",
      "url": "https://github.com/hodgef"
    }
  ],
  "autoupdate": {
