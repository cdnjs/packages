    "subtotal",
    "expand",
    "collapse",
    "summary"
  ],
  "license": "MIT",
  "homepage": "https://github.com/nagarajanchinnasamy/subtotal",
  "repository": {
    "type": "git",
    "url": "git://github.com/nagarajanchinnasamy/subtotal.git"
  },
  "authors": [
    {
      "name": "Nagarajan Chinnasamy"
    }
  ],
  "autoupdate": {
    "source": "npm",
    "target": "subtotal",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "*.@(css|js)"
        ]
      }
    ]
  },
  "filename": "subtotal.min.js"
}
