{
  "name": "bootstrap-fileinput",
  "filename": "js/fileinput.min.js",
  "description": "An enhanced HTML 5 file input for Bootstrap 3.x with file preview, multiple selection, ajax uploads, and more features.",
  "keywords": [
    "bootstrap",
    "file",
    "input",
    "preview",
    "image",
    "upload",
    "ajax",
    "multiple",
    "delete",
    "progress",
    "gallery"
  ],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/kartik-v/bootstrap-fileinput"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/kartik-v/bootstrap-fileinput.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "+(css|js)/fileinput*.+(css|js|map)",
          "js/plugins/*.+(js|map)",
          "js/locales/!(LANG.js)",
          "img/*.+(png|jpg|gif)",
          "themes/**/*"
        ]
      }
    ]
  },
  "author": "Kartik Visweswaran <kartikv2@gmail.com>"
}
