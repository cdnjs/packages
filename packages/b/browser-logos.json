{
  "name": "browser-logos",
  "description": "Collection of high resolution web browser logos with transparent backgrounds",
  "filename": "chromium/chromium_48x48.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/alrra/browser-logos.git"
  },
  "keywords": [
    "browser-logos",
    "high-resolution",
    "logos"
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/alrra/browser-logos.git",
    "fileMap": [
      {
        "basePath": "src",
        "files": [
          "**/!(*.+(json|md))"
        ]
      }
    ],
    "ignoreVersions": [
      "[a-z]*"
    ]
  },
  "license": "MIT"
}
