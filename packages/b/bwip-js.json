{
  "author": {
    "name": "Mark Warren <mwarren@metafloor.com>"
  },
  "description": "Barcode generator supporting over 90 types and standards.",
  "filename": "bwip-js-min.js",
  "homepage": "http://bwip-js.metafloor.com/",
  "keywords": [
    "qr",
    "itf",
    "gs1",
    "upc",
    "ean",
    "pdf417",
    "matrix",
    "data",
    "datamatrix",
    "qrcode",
    "databar",
    "code128",
    "code93",
    "code39",
    "code",
    "bar",
    "generator",
    "barcode"
  ],
  "license": "MIT",
  "name": "bwip-js",
  "repository": {
    "type": "git",
    "url": "https://github.com/metafloor/bwip-js.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "bwip-js",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "bwip*",
          "freetype.js"
        ]
      },
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      },
      {
        "basePath": "fonts",
        "files": [
          "**/*"
        ]
      }
    ]
  }
}
