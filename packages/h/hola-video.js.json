{
  "name": "hola-video.js",
  "description": "An HTML5 and Flash video player with a common API and skin for both. This is modified version to support HolaCDN.",
  "license": "Apache-2.0",
  "keywords": [
    "flash",
    "html5",
    "player",
    "video",
    "videojs"
  ],
  "homepage": "https://holacdn.com/player",
  "author": {
    "name": "Steve Heffernan"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hola/video.js.git"
  },
  "filename": "video.min.js",
  "autoupdate": {
    "source": "npm",
    "target": "@hola.org/video.js",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "!(examples)/*",
          "!(examples)"
        ]
      }
    ]
  }
}
