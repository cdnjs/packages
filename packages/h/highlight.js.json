{
  "name": "highlight.js",
  "filename": "highlight.min.js",
  "description": "Syntax highlighting with language auto-detection.",
  "homepage": "http://highlightjs.org",
  "keywords": [
    "highlight",
    "syntax highlighter"
  ],
  "autoupdate": {
    "source": "npm",
    "target": "@highlightjs/cdn-assets",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "highlight.min.js",
          "es/highlight.min.js",
          "es/core.min.js",
          "languages/*.min.js",
          "es/languages/*.min.js",
          "styles/*",
          "styles/**/*.css"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/highlightjs/highlight.js"
  },
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Josh Goebel",
      "email": "hello@joshgoebel.com"
    }
  ]
}
