{
  "name": "highlight.js",
  "filename": "highlight.min.js",
  "description": "Syntax highlighting with language autodetection.",
  "homepage": "http://highlightjs.org",
  "keywords": [
    "highlight",
    "syntax highlighter"
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/highlightjs/cdn-release.git",
    "fileMap": [
      {
        "basePath": "build",
        "files": [
          "highlight.min.js",
          "languages/*.min.js",
          "styles/*"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/highlightjs/highlight.js"
  },
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Ivan Sagalaev",
      "email": "maniac@softwaremaniacs.org"
    }
  ]
}
