{
  "name": "preact",
  "filename": "preact.min.js",
  "description": "Fast 3kb React alternative with the same ES6 API. Components & Virtual DOM.",
  "repository": {
    "type": "git",
    "url": "https://github.com/developit/preact"
  },
  "keywords": [
    "preact",
    "jsx",
    "vdom",
    "virtual dom",
    "components",
    "view"
  ],
  "homepage": "https://preactjs.com",
  "license": "MIT",
  "autoupdate": {
    "source": "npm",
    "target": "preact",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "!(*.+(flow|ts|mjs|mjs.map)|preact.esm.*)"
        ]
      },
      {
        "basePath": "hooks/dist",
        "files": [
          "!(*.+(flow|ts|mjs|mjs.map)|hooks.esm.*)"
        ]
      },
      {
        "basePath": "compat/dist",
        "files": [
          "!(*.+(flow|ts|mjs|mjs.map)|compat.esm.*)"
        ]
      }
    ]
  }
}
