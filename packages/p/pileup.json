{
  "name": "pileup",
  "description": "JavaScript track viewer",
  "filename": "pileup.min.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/hammerlab/pileup.js.git"
  },
  "keywords": [
    "genome",
    "track",
    "bam",
    "gene",
    "bioinformatics",
    "genomics",
    "sequencing",
    "reads",
    "interactive"
  ],
  "author": {
    "name": "Dan Vanderkam"
  },
  "license": "Apache-2.0",
  "autoupdate": {
    "source": "npm",
    "target": "pileup",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "!(main|test)/*pileup*.+(css|js|map)",
          "*pileup*"
        ]
      },
      {
        "basePath": "style",
        "files": [
          "pileup*.+(css|map)"
        ]
      }
    ]
  }
}
