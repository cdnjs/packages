{
  "name": "proteic",
  "filename": "proteic.js",
  "description": "Streaming and static data visualization for the modern web",
  "homepage": "https://proteic.js.org/",
  "keywords": [
    "d3",
    "chart",
    "data visualization",
    "visualization",
    "bigdata",
    "data-visualization",
    "dataviz"
  ],
  "author": "0xNacho",
  "repository": {
    "type": "git",
    "url": "https://github.com/proteus-h2020/proteic.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/proteus-h2020/proteic.git",
    "fileMap": [
      {
