{
  "name": "progressively",
  "description": "A JavaScript library to load images progressively",
  "homepage": "https://thinker3197.github.io/progressively",
  "repository": {
    "type": "git",
    "url": "https://github.com/thinker3197/progressively"
  },
  "author": {
    "name": "Ashish <@thinker3197> (https://thinker3197.github.io/)"
  },
  "license": "MIT",
  "filename": "progressively.min.js",
  "keywords": [
    "progressive",
    "lazy-load",
    "images"
  ],
  "autoupdate": {
    "source": "npm",
    "target": "progressively",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*.!(*gz)"
        ]
      },
      {
        "basePath": "src",
        "files": [
          "progressively*"
        ]
      }
    ]
  }
}
