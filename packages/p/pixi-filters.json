{
  "name": "pixi-filters",
  "description": "Optional display filters to work with Pixi.js",
  "author": "Mat Groves",
  "filename": "filters.min.js",
  "homepage": "https://pixijs.github.io/pixi-filters/docs/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pixijs/pixi-filters.git"
  },
  "keywords": [
    "pixi",
    "filter",
    "AsciiFilter",
    "BloomFilter",
    "ConvolutionFilter",
    "CrossHatchFilter",
    "DotFilter",
    "EmbossFilter",
    "PixelateFilter",
    "RGBSplitFilter",
    "ShockwaveFilter",
    "TiltShiftFilter",
    "TwistFilter"
  ],
  "autoupdate": {
    "source": "npm",
    "target": "pixi-filters",
    "fileMap": [
      {
        "basePath": "bin",
        "files": [
          "**/*"
        ]
      }
    ]
  }
}
