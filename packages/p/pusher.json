{
  "name": "pusher",
  "filename": "pusher.min.js",
  "description": "Pusher JavaScript library for the browser",
  "homepage": "http://pusher.com/",
  "keywords": [
    "pusher",
    "websockets",
    "realtime"
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/pusher/pusher-js.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "react-native/**/*"
        ]
      },
      {
        "basePath": "dist/worker",
        "files": [
          "**/*"
        ]
      },
      {
        "basePath": "dist/web",
        "files": [
          "**/*"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pusher/pusher-js"
  },
  "license": "MIT",
  "author": "Pusher"
}
