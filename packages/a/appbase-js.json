{
  "name": "appbase-js",
  "description": "Appbase.io streaming client lib for Javascript",
  "filename": "appbase.js",
  "keywords": [
    "appbase",
    "elasticsearch",
    "stream",
    "search"
  ],
  "license": "ISC",
  "homepage": "http://docs.appbase.io/scalr/javascript/nodejs-intro.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/appbaseio/appbase-js.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "appbase-js",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/!(*.gz|*.br|*.es.js|*.cjs.js)"
        ]
      }
    ],
    "ignoreVersions": [
      "1.0.0"
    ]
  },
  "authors": [
    {
      "name": "Sacheendra Talluri"
    }
  ]
}
