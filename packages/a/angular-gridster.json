{
  "name": "angular-gridster",
  "description": "An implementation of gridster-like widgets for Angular JS. This directive gives you gridster behavior",
  "filename": "angular-gridster.min.js",
  "keywords": [
    "angular",
    "widget",
    "gridster-like",
    "angular directives"
  ],
  "author": "https://github.com/ManifestWebDesign/angular-gridster/graphs/contributors",
  "homepage": "https://manifestwebdesign.github.io/angular-gridster",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ManifestWebDesign/angular-gridster.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/ManifestWebDesign/angular-gridster.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*gridster*"
        ]
      },
      {
        "basePath": "src",
        "files": [
          "angular-gridster*.+(css|js|map)"
        ]
      },
      {
        "basePath": "app",
        "files": [
          "gridster*"
        ]
      }
    ]
  },
  "license": "MIT"
}
