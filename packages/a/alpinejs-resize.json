  "keywords": [
    "framework",
    "reactive",
    "declarative",
    "plugin"
  ],
  "license": "MIT",
  "homepage": "https://alpinejs.dev/plugins/resize",
  "repository": {
    "type": "git",
    "url": "git://github.com/alpinejs/alpine.git"
  },
  "authors": [
    {
      "name": "Caleb Porzio"
    }
  ],
  "autoupdate": {
    "source": "npm",
    "target": "@alpinejs/resize",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "*.js"
        ]
      }
    ]
  },
  "filename": "cdn.js"
}
