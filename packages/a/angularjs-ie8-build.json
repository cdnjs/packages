{
  "name": "angularjs-ie8-build",
  "filename": "angular.min.js",
  "description": "A build of AngularJS 1.3 with polyfils and some changes to bring back IE8 compatibilty",
  "author": "Fergal Doyle",
  "keywords": [
    "angular",
    "angular 1.3",
    "angular 1.3 ie8",
    "angular 1.4",
    "angular 1.4 ie8",
    "ie8"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fergaldoyle/angular.js-ie8-builds.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "angularjs-ie8-build",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/!(*.json)"
        ]
      }
    ]
  }
}
