{
  "name": "angulartics",
  "description": "Vendor-agnostic web analytics for AngularJS applications",
  "filename": "angulartics.min.js",
  "homepage": "http://angulartics.github.io",
  "repository": {
    "type": "git",
    "url": "git://github.com/angulartics/angulartics.git"
  },
  "keywords": [
    "angular",
    "analytics",
    "tracking",
    "google analytics",
    "google tag manager",
    "mixpanel",
    "kissmetrics",
    "chartbeat",
    "woopra",
    "segment.io",
    "splunk",
    "flurry",
    "piwik",
    "adobe analytics",
    "omniture",
    "page tracking",
    "event tracking",
    "scroll tracking"
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/angulartics/angulartics.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "*"
        ]
      }
    ],
    "ignoreVersions": [
      "googleanalytics"
    ]
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Luis Farzati",
      "email": "lfarzati@gmail.com",
      "url": "https://github.com/angulartics"
    }
  ]
}
