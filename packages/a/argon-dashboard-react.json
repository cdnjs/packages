  "keywords": [
    "react",
    "reactjs",
    "argon",
    "argon-react",
    "dashboard",
    "dashboard-react",
    "argon-dashboard",
    "argon-dashboard-react"
  ],
  "license": "MIT",
  "homepage": "https://demos.creative-tim.com/argon-dashboard-react/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/creativetimofficial/argon-dashboard-react.git"
  },
  "authors": [
    {
      "name": "Creative Tim"
    }
  ],
  "autoupdate": {
    "source": "npm",
    "target": "argon-dashboard-react",
    "fileMap": [
      {
        "basePath": "src",
