{
  "name": "videojs-vimeo",
  "description": "Allows you to use Vimeo URL as source with Video.js.",
  "keywords": [
    "video.js",
    "vimeo",
    "plugin"
  ],
  "author": "Benoit Tremblay <trembl.ben@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/videojs/videojs-vimeo#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/videojs/videojs-vimeo.git"
  },
  "filename": "video.js",
  "autoupdate": {
    "source": "npm",
    "target": "videojs-vimeo",
    "fileMap": [
      {
        "basePath": "lib",
        "files": [
          "*.@(js|css|swf)",
          "font/*.*"
        ]
      }
    ]
  }
}
