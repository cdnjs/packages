{
  "name": "inferno-hyperscript",
  "filename": "inferno-hyperscript.min.js",
  "license": "MIT",
  "description": "Bridging hyperscript to InfernoJS",
  "homepage": "https://infernojs.org/",
  "keywords": [
    "inferno",
    "hyperscript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infernojs/inferno.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "inferno-hyperscript",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/inferno-hyperscript!(*.node)*.!(*ts)"
        ]
      }
    ],
    "ignoreVersions": [
      "*-alpha.*"
    ]
  },
  "authors": [
    {
      "name": "Terin Stock",
      "email": "terinjokes@gmail.com",
      "url": "https://terinstock.com/"
    }
  ]
}
