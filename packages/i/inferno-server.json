{
  "name": "inferno-server",
  "filename": "inferno-server.min.js",
  "license": "MIT",
  "description": "Provides methods to render HTML strings from Inferno elements",
  "homepage": "https://infernojs.org/",
  "keywords": [
    "babel",
    "react",
    "inferno",
    "framework",
    "interfaces",
    "user interfaces",
    "html",
    "renderToString",
    "server",
    "dom",
    "browser",
    "rollup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/infernojs/inferno.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "inferno-server",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/inferno-server!(*.node)*.!(*ts)"
        ]
      }
    ],
    "ignoreVersions": [
      "*-alpha.*"
    ]
  },
  "authors": [
    {
      "name": "Dominic Gannaway",
      "email": "dg@domgan.com"
    }
  ]
}
