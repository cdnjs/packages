    "immutable",
    "mutable",
    "copy-on-write"
  ],
  "author": {
    "name": "Michel Weststrate"
  },
  "license": "MIT",
  "homepage": "https://github.com/immerjs/immer#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/immerjs/immer.git"
  },
  "filename": "immer.umd.production.min.js",
  "autoupdate": {
    "source": "npm",
    "target": "immer",
    "fileMap": [
      {
