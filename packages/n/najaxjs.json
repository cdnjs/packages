{
  "name": "najaxjs",
  "filename": "najax.min.js",
  "description": "Ajax simple library. You can use ajax easily and customize request & response behaviors.",
  "license": "MIT",
  "keywords": [
    "any",
    "any-js",
    "ajax",
    "json",
    "jsonp",
    "xhr",
    "async",
    "sync",
    "http"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/any-js/najaxjs/"
  },
  "homepage": "https://any-js.github.io/",
  "autoupdate": {
    "source": "npm",
    "target": "najaxjs",
    "fileMap": [
      {
        "basePath": "src",
        "files": [
          "+(najax|rlk)*"
        ]
      },
      {
        "basePath": "bin",
        "files": [
          "+(najax|rlk)*"
        ]
      }
    ]
  },
  "authors": [
    {
      "name": "anyjser"
    }
  ]
}
