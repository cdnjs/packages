{
  "name": "open-weather",
  "filename": "openweather.min.js",
  "description": "A simple, lightweight jQuery plugin used to display the current weather of any city using the free OpenWeatherMap API.",
  "keywords": [
    "weather",
    "temperature",
    "wind",
    "speed",
    "humidity",
    "sunset",
    "sunrise",
    "city"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/michael-lynch/open-weather.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/michael-lynch/open-weather.git",
    "fileMap": [
      {
        "basePath": "build",
        "files": [
          "**/*"
        ]
      },
      {
        "basePath": "src",
        "files": [
          "openweather*.+(js|map)"
        ]
      }
    ]
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Lynch",
      "url": "http://michaelynch.com"
    }
  ]
}
