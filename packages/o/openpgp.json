{
  "name": "openpgp",
  "filename": "openpgp.min.js",
  "description": "OpenPGP.js is a Javascript implementation of the OpenPGP protocol. This is defined in RFC 4880.",
  "homepage": "http://openpgpjs.org/",
  "keywords": [
    "crypto",
    "pgp",
    "gpg",
    "openpgp"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/openpgpjs/openpgpjs"
  },
  "license": "LGPL-3.0",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/openpgpjs/openpgpjs.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      }
    ]
  },
  "authors": [
    {
      "name": "OpenPGP Development Team",
      "email": "list@openpgpjs.org",
      "url": "https://github.com/openpgpjs/openpgpjs/graphs/contributors"
    }
  ]
