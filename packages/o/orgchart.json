{
  "name": "orgchart",
  "description": "Simple and direct organization chart(tree-like hierarchy) plugin based on pure DOM and jQuery.",
  "filename": "js/jquery.orgchart.min.js",
  "keywords": [
    "jquery",
    "plugin",
    "organization",
    "chart",
    "orgchart",
    "tree",
    "tree-like",
    "tree-view"
  ],
  "author": "dabeng <dabeng413@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/dabeng/OrgChart.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/dabeng/OrgChart.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      },
      {
        "basePath": "src",
        "files": [
          "+(css|js)/jquery.orgchart*"
        ]
      },
      {
        "basePath": "",
        "files": [
          "jquery.orgchart*.+(js|css|map)"
        ]
      }
    ]
  },
