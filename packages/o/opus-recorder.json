{
  "author": "Chris Rudmin",
  "description": "A library for recording opus encoded audio",
  "filename": "recorder.min.js",
  "keywords": [
    "Ogg",
    "Opus",
    "PCM",
    "Decoder",
    "Resampler",
    "Streamer",
    "Audio",
    "Microphone",
    "Recorder",
    "Wav",
    "Wave",
    "WebAssembly"
  ],
  "license": "MIT",
  "name": "opus-recorder",
  "repository": {
    "type": "git",
    "url": "git://github.com/chris-rudmin/opus-recorder.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "opus-recorder",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      },
      {
        "basePath": "dist-unminified",
        "files": [
          "**/*"
        ]
      },
      {
        "basePath": "",
        "files": [
          "libopus.js"
        ]
      }
    ]
  }
}
