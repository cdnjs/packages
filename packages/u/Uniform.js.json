{
  "name": "Uniform.js",
  "filename": "js/jquery.uniform.bundled.js",
  "homepage": "http://uniformjs.com",
  "description": "A plugin for jQuery. Make your form controls look how you want them to.",
  "keywords": [
    "uniform"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/pixelmatrix/uniform.git"
  },
  "license": "MIT",
  "autoupdate": {
    "source": "npm",
    "target": "jquery.uniform",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      },
      {
        "basePath": "",
        "files": [
          "themes/**/*.+(png|css)"
        ]
      },
      {
        "basePath": "lib",
        "files": [
          "jquery.uniform.js"
        ]
      }
    ]
  }
}
