{
  "name": "xterm",
  "filename": "xterm.js",
  "description": "Build terminals in the browser",
  "homepage": "https://xtermjs.org/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xtermjs/xterm.js"
  },
  "keywords": [
    "xterm",
    "terminal",
    "browser-tty"
  ],
  "license": "MIT",
  "autoupdate": {
    "source": "npm",
    "target": "@xterm/xterm",
    "fileMap": [
      {
        "basePath": "lib",
        "files": [
          "*.js?(.map)"
        ]
      },
      {
        "basePath": "css",
        "files": [
          "*.css"
        ]
      }
    ]
  },
  "optimization": {
    "js": false
  },
  "authors": [
    {
      "name": "Daniel Imms"
    }
  ]
}
