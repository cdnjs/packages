{
  "name": "tabler-icons",
  "description": "A set of over 500 free MIT-licensed high-quality SVG icons for you to use in your web projects. Each icon is designed on a 24x24 grid and a `2px` stroke.",
  "keywords": [
    "icons",
    "iconset",
    "svg-icons",
    "svg",
    "icons-pack",
    "bootstrap-icons",
    "css",
    "icon-pack"
  ],
  "license": "MIT",
  "homepage": "https://github.com/tabler/tabler-icons#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tabler/tabler-icons.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "tabler-icons",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "tabler-*.svg",
          "icons/*.svg",
          "iconfont/tabler-*.@(css|scss)",
          "iconfont/fonts/tabler-*"
        ]
      }
    ]
  },
  "filename": "iconfont/tabler-icons.min.css",
  "authors": [
    {
      "name": "Pawe≈Ç Kuna",
      "url": "https://github.com/codecalm"
    }
  ]
}
