  "keywords": [
    "datepicker",
    "datetimepicker",
    "timepicker"
  ],
  "license": "MIT",
  "homepage": "https://getdatepicker.com/",
  "repository": {
    "type": "git",
    "url": "git://github.com/Eonasdan/tempus-dominus.git"
  },
  "authors": [
    {
      "name": "Jonathan Peterson"
    }
  ],
  "autoupdate": {
