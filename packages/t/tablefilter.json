{
  "name": "tablefilter",
  "description": "A Javascript library making HTML tables filterable and a bit more",
  "keywords": [
    "filter",
    "table",
    "javascript",
    "filterable",
    "grid",
    "datagrid",
    "sort",
    "pagination"
  ],
  "license": "MIT",
  "homepage": "https://www.tablefilter.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koalyptus/TableFilter.git"
  },
  "authors": [
    {
      "name": "Max Guglielmi",
      "url": "https://github.com/koalyptus"
    }
  ],
  "autoupdate": {
    "source": "npm",
    "target": "tablefilter",
    "fileMap": [
      {
        "basePath": "dist/tablefilter",
        "files": [
          "*.js",
          "style/*.css",
          "style/themes/**/*.@(css|png|gif|jpg)"
        ]
      }
    ]
  },
  "filename": "tablefilter.js"
}
