    "keywords": [
        "graph",
        "graphs",
        "node",
        "blueprints"
    ],
    "license": "MIT",
    "filename": "build/litegraph.min.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/jagenjo/litegraph.js"
    },
    "autoupdate": {
        "source": "npm",
        "target": "litegraph.js",
        "fileMap": [
