    "font",
    "awesome",
    "icons",
    "icons8",
    "line-awesome",
    "font-awesome"
  ],
  "author": {
    "name": "Icons8"
  },
  "license": "MIT",
  "homepage": "https://github.com/icons8/line-awesome#readme",
  "repository": {
    "type": "git",
