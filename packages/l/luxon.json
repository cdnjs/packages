{
  "name": "luxon",
  "description": "Immutable date wrapper",
  "keywords": [
    "date",
    "immutable"
  ],
  "author": {
    "name": "Isaac Cambron"
  },
  "license": "MIT",
  "homepage": "https://github.com/moment/luxon#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moment/luxon.git"
  },
  "filename": "luxon.min.js",
  "autoupdate": {
    "source": "npm",
    "target": "luxon",
    "fileMap": [
      {
        "basePath": "build/global",
        "files": [
          "*.@(js|map)"
        ]
      },
      {
        "basePath": "build",
        "files": [
          "cjs-browser/*.@(js|map)"
        ]
      }
    ]
  }
}
