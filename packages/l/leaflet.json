{
  "name": "leaflet",
  "filename": "leaflet.js",
  "description": "JavaScript library for mobile-friendly interactive maps",
  "homepage": "http://leafletjs.com/",
  "keywords": [
    "maps",
    "mobile"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Leaflet/Leaflet.git"
  },
  "license": "BSD-2-Clause",
  "autoupdate": {
    "source": "npm",
    "target": "leaflet",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/!(*.+(json|html)|*-src.esm.js?(.map))"
        ]
      }
    ]
  }
}
