{
  "description": "Debug utility with markdown support that runs on browser and server",
  "filename": "logdown.min.js",
  "homepage": "https://caiogondim.github.io/logdown.js/",
  "keywords": [
    "log",
    "markdown",
    "debug",
    "console",
    "info",
    "error",
    "logging"
  ],
  "license": "MIT",
  "name": "logdown",
  "repository": {
    "type": "git",
    "url": "https://github.com/caiogondim/logdown"
  },
  "autoupdate": {
    "source": "npm",
    "target": "logdown",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/!(*.gzip|*.ts)"
        ]
      }
    ]
  },
  "authors": [
    {
      "name": "Caio Gondim <me@caiogondim.com> (https://caiogondim.com)"
    }
  ]
}
