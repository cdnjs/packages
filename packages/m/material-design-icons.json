{
  "name": "material-design-icons",
  "description": "Material Design icons by Google",
  "filename": "iconfont/material-icons.min.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/material-design-icons"
  },
  "keywords": [
    "icons",
    "material",
    "material-design",
    "google"
  ],
  "license": "Apache-2.0",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/google/material-design-icons.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "**/*.css",
          "**/*.eot",
          "**/*.ttf",
          "**/*.woff",
          "**/*.woff2",
          "**/*.png",
          "**/*.svg"
        ]
      }
    ],
    "ignoreVersions": [
      "2.0"
    ]
  },
  "authors": [
    {
      "name": "Material Design Authors"
    }
  ],
  "optimization": {
    "png": false
  }
}
