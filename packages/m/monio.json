{
  "name": "monio",
  "description": "The most powerful IO monad implementation in JS, possibly in any language!",
  "filename": "umd/bundle.js",
  "keywords": [
    "fp",
    "monad",
    "async",
    "io",
    "promise",
    "observable"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/getify/monio.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/getify/monio",
  "autoupdate": {
    "source": "npm",
    "target": "monio",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "umd/*.?(js|json|map)",
          "umd/lib/*.?(js|json|map)",
          "esm/*.?(js|mjs|json|map)",
          "esm/lib/*.?(js|mjs|json|map)"
        ]
      }
    ]
  },
  "optimization": {
    "js": false
  },
  "authors": [
    {
      "name": "Kyle Simpson",
      "email": "getify@gmail.com"
    }
  ]
}
