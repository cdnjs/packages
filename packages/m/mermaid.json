{
  "name": "mermaid",
  "filename": "mermaid.min.js",
  "description": "Markdownish syntax for generating flowcharts, sequence diagrams, class diagrams and gantt charts.",
  "keywords": [
    "diagram",
    "markdown",
    "flowchart",
    "sequence diagram",
    "gantt",
    "class diagram",
    "git graph"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/knsv/mermaid"
  },
  "license": "MIT",
  "author": {
    "name": "Knut Sveidqvist"
  },
  "autoupdate": {
    "source": "npm",
    "target": "mermaid",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/!(*.html)"
        ]
      }
    ]
  }
}
