{
  "description": "Marqueefy is a custom Marquee component used to create horizontal or vertical scrolling content.",
  "filename": "js/marqueefy.min.js",
  "homepage": "https://marqueefy.github.io/",
  "keywords": [
    "marqueefy",
    "marquee",
    "custom-marquee",
    "html-marquee",
    "scrolling-text",
    "scrolling-content"
  ],
  "license": "MIT",
  "name": "marqueefy",
