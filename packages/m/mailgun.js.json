{
  "name": "mailgun.js",
  "description": "A javascript sdk for Mailgun built with webpack, babel & es6. This can be used in node or in the browser*.",
  "keywords": [
    "mailgun",
    "email"
  ],
  "author": {
    "name": "Mailgun"
  },
  "license": "MIT",
  "homepage": "https://github.com/mailgun/mailgun-js#readme",
  "repository": {
    "type": "git",
    "url": "git://github.com/mailgun/mailgun-js.git"
  },
  "filename": "mailgun.min.js",
  "autoupdate": {
    "source": "npm",
    "target": "mailgun.js",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "*.@(js|map)"
        ]
      }
    ]
  }
}
