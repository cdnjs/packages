{
  "name": "qoopido.js",
  "description": "Modular library of dry, extendable and AMD compatible JavaScript modules",
  "repository": {
    "type": "git",
    "url": "git://github.com/dlueth/qoopido.js.git"
  },
  "license": "(MIT OR GPL)",
  "keywords": [
    "modular",
    "library",
    "toolit",
    "amd",
    "require",
    "dry"
  ],
  "filename": "base.js",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/dlueth/qoopido.js.git",
    "fileMap": [
      {
        "basePath": "dist/latest/min",
        "files": [
          "**/*.js"
        ]
      }
    ],
    "ignoreVersions": [
      "support",
      "widget-adapt"
    ]
  },
  "authors": [
    {
      "name": "Dirk Lueth",
      "email": "info@qoopido.com"
    }
  ]
}
