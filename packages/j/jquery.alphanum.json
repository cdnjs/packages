{
  "name": "jquery.alphanum",
  "filename": "jquery.alphanum.min.js",
  "description": "Restrict what characters that may be entered into text fields. Supports alphanumeric, alphabetic or numeric. Easy to use. Fully customisable. Cross Browser compatible.",
  "keywords": [
    "alphanumeric",
    "alphanun",
    "alphabetic",
    "numeric",
    "input",
    "textarea",
    "text",
    "restrict"
  ],
  "license": "GPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinSheedy/jquery.alphanum"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/KevinSheedy/jquery.alphanum.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "jquery.alphanum*.js"
        ]
      }
    ],
    "ignoreVersions": [
      "Stable_1.0",
      "Stable_1.1"
    ]
  },
  "authors": [
    {
      "name": "Kevin Sheedy"
    }
  ]
}
