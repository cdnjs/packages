{
  "name": "jqgrid",
  "filename": "js/jquery.jqGrid.min.js",
  "description": "jqGrid is an Ajax-enabled JavaScript control that provides solutions for representing and manipulating tabular data on the web.",
  "homepage": "http://www.guriddo.net",
  "keywords": [
    "grid",
    "jquery"
  ],
  "license": "CC-BY-NC-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tonytomov/jqGrid"
  },
  "author": {
    "name": "Tony Tomov",
    "url": "https://www.guriddo.net",
    "email": "tony@trirand.com"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/tonytomov/jqGrid.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "css/**/ui.jqgrid*.+(css|map)",
          "js/**/+(jquery.jqGrid|grid|jquery.fmatter|jquery.sortable|jqDnR|jqModal)*.+(js|map)",
          "js/minified/+(jqDnR|jqModal)*.+(js|map)",
          "plugins/+(grid*|searchFilter*).+(js|css|map)"
        ]
      }
    ]
  }
}
