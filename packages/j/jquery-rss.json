{
  "name": "jquery-rss",
  "filename": "jquery.rss.min.js",
  "description": "An easy-to-use rss plugin for jquery with templating.",
  "keywords": [
    "rss",
    "jquery",
    "ender"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sdepold/jquery-rss.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/sdepold/jquery-rss.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/!(*.jar)"
        ]
      },
      {
        "basePath": "src",
        "files": [
          "jquery.rss*"
        ]
      }
    ]
  },
  "authors": [
    {
      "name": "Sascha Depold",
      "email": "sascha@depold.com",
      "url": "http://depold.com"
    }
  ]
}
