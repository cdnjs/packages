{
  "name": "jquery-migrate",
  "filename": "jquery-migrate.min.js",
  "description": "Migrate older jQuery code to jQuery 1.9+",
  "homepage": "http://jquery.com/",
  "keywords": [
    "framework",
    "toolkit",
    "popular"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jquery/jquery-migrate.git"
  },
  "license": "MIT",
  "author": {
    "name": "jQuery Foundation and other contributors",
    "url": "https://github.com/jquery/jquery-migrate/blob/master/AUTHORS.txt"
  },
  "autoupdate": {
    "source": "npm",
    "target": "jquery-migrate",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      }
    ]
  }
}
