{
  "name": "jsnlog",
  "description": "Simple logging package. Use on client and/or server. On the client, sends logs to the server and stores them in your existing server side logs. On the server, log to console or use Winston transports. Ideal for logging exceptions, AJAX timeouts.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mperdeck/jsnlog.js.git"
  },
  "keywords": [
    "logging",
    "client",
    "exceptions",
    "ajax",
    "timeouts",
    "debugging",
    "winston"
  ],
  "author": {
    "name": "Matt Perdeck"
  },
  "license": "MIT",
  "homepage": "http://nodejs.jsnlog.com/",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/mperdeck/jsnlog.js.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "jsnlog.min.js",
          "jsnlog.js.map",
          "jsnlog.js"
        ]
      }
    ]
  },
  "filename": "jsnlog.min.js"
}
