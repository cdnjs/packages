    "PKCS#1",
    "PKCS#5",
    "PKCS#8",
    "private key",
    "public key",
    "CSR",
    "PKCS#10",
    "hash function",
    "HMac",
    "ASN.1",
    "certificate",
    "X.509",
    "RFC 3161",
    "Digital Timestamp",
    "Timestamp",
    "Time Stamp Token",
    "CMS",
    "Cryptgraphic Message Syntax",
    "PKCS#7",
    "Signature",
    "Digital Signature",
    "signing",
    "Message Digest",
    "JSON Web Token",
    "JWT",
    "JSON Web Signature",
    "JWS",
    "JSON Web Key",
    "JWK",
    "JOSE",
    "JWA"
  ],
  "author": "Kenji Urushima",
  "license": "MIT",
  "homepage": "https://kjur.github.io/jsrsasign/",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/kjur/jsrsasign.git",
    "fileMap": [
      {
