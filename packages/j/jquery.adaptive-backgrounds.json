{
  "name": "jquery.adaptive-backgrounds",
  "author": "Brian Gonzalez",
  "description": "A simple jQuery plugin to extract the dominant color of an image and apply it to the background of its parent element.",
  "repository": {
    "type": "git",
    "url": "https://github.com/briangonzalez/jquery.adaptive-backgrounds.js.git"
  },
  "filename": "jquery.adaptive-backgrounds.min.js",
  "homepage": "https://briangonzalez.github.io/jquery.adaptive-backgrounds.js/",
  "license": "MIT",
  "keywords": [
    "jquery",
    "adaptive",
    "background",
    "color",
    "image",
    "processing"
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/briangonzalez/jquery.adaptive-backgrounds.js.git",
    "fileMap": [
      {
        "basePath": "src",
        "files": [
          "jquery.adaptive-backgrounds*"
        ]
      },
      {
        "basePath": "dist",
        "files": [
          "**/*"
        ]
      }
    ]
  }
}
