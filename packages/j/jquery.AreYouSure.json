{
  "name": "jquery.AreYouSure",
  "description": "A light-weight jQuery \"dirty forms\" Plugin - it monitors HTML forms and alerts users to unsaved changes if they attempt to close the browser or navigate away from the page. (Are you sure?)",
  "filename": "jquery.are-you-sure.min.js",
  "author": {
    "name": "Chris Dance <chris.dance@papercut.com> (https://github.com/codedance)"
  },
  "license": "MIT/GPLv2",
  "keywords": [
    "dirty",
    "form",
    "onbeforeunload",
    "save",
    "check"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/codedance/jquery.AreYouSure"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/codedance/jquery.AreYouSure.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "jquery.are-you-sure*.js"
        ]
      }
    ]
  }
}
