{
  "name": "remarkable",
  "description": "Markdown parser, done right. 100% Commonmark support, extensions, syntax plugins, high speed - all in one.",
  "repository": {
    "type": "git",
    "url": "git://github.com/jonschlinkert/remarkable.git"
  },
  "license": "MIT",
  "filename": "remarkable.min.js",
  "keywords": [
    "md",
    "markdown",
    "process",
    "text",
    "parse",
    "parser",
    "render",
    "renderer",
    "commonmark"
  ],
  "autoupdate": {
    "source": "npm",
    "target": "remarkable",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "**/remarkable*"
        ]
      }
    ]
  }
}
