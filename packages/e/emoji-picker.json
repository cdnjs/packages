{
  "name": "emoji-picker",
  "filename": "js/emoji-picker.min.js",
  "description": "Add a slick emoji selector to input fields and textareas on your website.",
  "repository": {
    "type": "git",
    "url": "https://github.com/OneSignal/emoji-picker"
  },
  "license": "MIT",
  "homepage": "https://onesignal.github.io/emoji-picker/",
  "keywords": [
    "emoji",
    "jquery-emoji",
    "textarea",
    "input",
    "emoticons",
    "icons",
    "jquery-plugin"
  ],
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/OneSignal/emoji-picker.git",
    "fileMap": [
      {
        "basePath": "lib",
        "files": [
          "img/*.+(png|gif)",
          "+(css|js)/+(emoji|jquery.emojiarea|util|config)*.+(css|js|map)"
        ]
      }
    ]
  },
  "authors": [
    {
      "name": "Jason Pang @ OneSignal",
      "url": "https://onesignal.com"
    }
  ]
}
