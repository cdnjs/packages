  "filename": "index.css",
  "keywords": [
    "fontsource",
    "font",
    "font family",
    "google fonts",
    "dseg7-modern",
    "DSEG7 Modern",
    "css",
    "sass",
    "front-end",
    "web",
    "typeface",
    "variable"
  ],
  "authors": [
    {
      "name": "keshikan",
      "url": "https://www.keshikan.net"
    }
  ],
  "license": "OFL-1.1",
  "homepage": "https://fontsource.org/fonts/dseg7-modern",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fontsource/font-files.git"
  },
  "autoupdate": {
    "excludeFromSearch": true,
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "*.css",
          "files/*.@(woff|woff2)"
        ]
      }
