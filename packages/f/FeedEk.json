    "reader",
    "parser",
    "jQuery",
    "YQL",
    "YQL API"
  ],
  "author": {
    "name": "Engin KIZIL",
    "url": "http://enginkizil.com"
  },
  "license": "MIT",
  "homepage": "http://jquery-plugins.net/FeedEk/FeedEk.html",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/enginkizil/FeedEk.git",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "js/FeedEk*.js",
          "css/FeedEk*.css"
        ]
      }
    ]
  }
}
