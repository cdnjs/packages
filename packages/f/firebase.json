{
  "description": "Firebase JavaScript library for web and Node.js",
  "filename": "firebase.js",
  "homepage": "https://firebase.google.com/",
  "keywords": [
    "authentication",
    "database",
    "Firebase",
    "firebase",
    "realtime",
    "storage"
  ],
  "license": "Apache-2.0",
  "name": "firebase",
  "repository": {
    "type": "git",
    "url": "https://github.com/firebase/firebase-js-sdk.git"
  },
  "autoupdate": {
    "source": "npm",
    "target": "firebase",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "firebase*.+(js|map)"
        ]
      },
      {
        "basePath": "lib",
        "files": [
          "firebase*.+(js|map)"
        ]
      },
      {
        "basePath": "dist",
        "files": [
          "firebase*.+(js|map)"
        ]
      }
    ],
    "ignoreVersions": [
      "*-canary.*"
    ]
  },
  "authors": [
    {
      "name": "Firebase",
      "email": "firebase-support@google.com",
      "url": "https://firebase.google.com/"
    }
  ]
}
