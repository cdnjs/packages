{
  "name": "ffmpeg-core",
  "filename": "ffmpeg-core.js",
  "description": "A pure Webassembly / Javascript port of FFmpeg.",
  "keywords": [
    "ffmpeg"
  ],
  "license": "LGPL-2.0",
  "autoupdate": {
    "source": "npm",
    "target": "@ffmpeg/core",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "{,umd/,esm/}*@(.js?(.map)|.wasm)"
        ]
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ffmpegwasm/ffmpeg.wasm-core"
  }
}
