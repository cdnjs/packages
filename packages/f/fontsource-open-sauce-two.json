{
  "name": "fontsource-open-sauce-two",
  "description": "Self-host the Open Sauce Two font in a neatly bundled NPM package.",
  "filename": "index.css",
  "keywords": [
    "fontsource",
    "font",
    "font family",
    "google fonts",
    "open-sauce-two",
    "Open Sauce Two",
    "css",
    "sass",
    "front-end",
    "web",
    "typeface",
    "variable"
  ],
  "authors": [
    {
      "name": "The Open Sauce Two Authors",
      "url": "https://github.com/marcologous/Open-Sauce-Fonts"
    }
  ],
  "license": "OFL-1.1",
  "homepage": "https://fontsource.org/fonts/open-sauce-two",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fontsource/font-files.git"
  },
  "autoupdate": {
    "excludeFromSearch": true,
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "*.css",
          "files/*.@(woff|woff2)"
        ]
      }
    ],
    "source": "npm",
    "target": "@fontsource/open-sauce-two"
  }
}
