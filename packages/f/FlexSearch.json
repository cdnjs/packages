{
  "name": "FlexSearch",
  "description": "Next-Generation full text search library with zero dependencies.",
  "author": "Thomas Wilkerling",
  "license": "Apache-2.0",
  "keywords": [
    "search",
    "fulltext",
    "fastest",
    "contextual",
    "fuzzy",
    "matching",
    "scoring",
    "indexing",
    "fieldsearch"
  ],
  "filename": "flexsearch.min.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/nextapps-de/flexsearch.git"
  },
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/nextapps-de/flexsearch.git",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "!(flexsearch.node.js|flexsearch.pre.js)"
        ]
      }
    ]
  }
}
