{
  "name": "fontsource-bluu-next",
  "description": "Self-host the Bluu Next font in a neatly bundled NPM package.",
  "filename": "index.css",
  "keywords": [
    "fontsource",
    "font",
    "font family",
    "google fonts",
    "bluu-next",
    "Bluu Next",
    "css",
    "sass",
    "front-end",
    "web",
    "typeface",
    "variable"
  ],
  "authors": [
    {
      "name": "Jean-Baptiste Morizot,",
      "url": "http://cargocollective.com/jbmrz"
    }
  ],
  "license": "OFL-1.1",
  "homepage": "https://fontsource.org/fonts/bluu-next",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fontsource/font-files.git"
  },
  "autoupdate": {
    "excludeFromSearch": true,
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "*.css",
          "files/*.@(woff|woff2)"
        ]
      }
    ],
    "source": "npm",
    "target": "@fontsource/bluu-next"
  }
}
