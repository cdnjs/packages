{
  "name": "dropzone",
  "filename": "min/dropzone.min.js",
  "description": "Handles drag and drop of files for you.",
  "homepage": "http://www.dropzonejs.com/",
  "keywords": [
    "html5",
    "file",
    "upload"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/enyo/dropzone.git"
  },
  "license": "MIT",
  "autoupdate": {
    "source": "npm",
    "target": "dropzone",
    "fileMap": [
      {
        "basePath": "dist",
        "files": [
          "*.js",
          "*.css",
          "*/*.js",
          "*/*.css"
        ]
      }
    ]
  }
}
