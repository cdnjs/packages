{
  "name": "dc",
  "description": "A multi-dimensional charting library built to work natively with crossfilter and rendered using d3.js",
  "homepage": "http://dc-js.github.io/dc.js/",
  "keywords": [
    "visualization",
    "svg",
    "animation",
    "canvas",
    "chart",
    "dimensional",
    "crossfilter",
    "d3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dc-js/dc.js.git"
  },
  "license": "Apache-2.0",
  "filename": "dc.min.js",
  "autoupdate": {
    "source": "npm",
    "target": "dc",
    "fileMap": [
      {
        "basePath": "",
        "files": [
          "dc*.@(js|css|map)"
        ]
      },
      {
        "basePath": "dist",
        "files": [
          "**/*.@(js|css|map)"
        ]
      }
    ]
  },
  "authors": [
    {
      "name": "Nick Zhu",
      "url": "http://nzhu.blogspot.ca/"
    }
  ]
}
